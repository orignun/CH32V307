-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
LCD_LVGL.hex \

SECONDARY_LIST += \
LCD_LVGL.lst \

SECONDARY_SIZE += \
LCD_LVGL.siz \


# 所有目标
all: LCD_LVGL.elf secondary-outputs

# 工具调用
LCD_LVGL.elf: $(OBJS) $(USER_OBJS)
	@	@	riscv-none-embed-gcc -march=rv32imac -mabi=ilp32 -msmall-data-limit=8 -mno-save-restore -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -Wunused -Wuninitialized  -g -T "C:\Users\zhengshengbing\Desktop\opench-ch32v307-master\firmware\SRC\Ld\Link.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"LCD_LVGL.map" --specs=nano.specs --specs=nosys.specs -o "LCD_LVGL.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@	@
LCD_LVGL.hex: LCD_LVGL.elf
	@	riscv-none-embed-objcopy -O ihex "LCD_LVGL.elf"  "LCD_LVGL.hex"
	@	@
LCD_LVGL.lst: LCD_LVGL.elf
	@	riscv-none-embed-objdump --all-headers --demangle --disassemble "LCD_LVGL.elf" > "LCD_LVGL.lst"
	@	@
LCD_LVGL.siz: LCD_LVGL.elf
	@	riscv-none-embed-size --format=berkeley "LCD_LVGL.elf"
	@	@
# 其他目标
clean:
	-$(RM) $(ASM_UPPER_DEPS)$(OBJS)$(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_DEPS)$(S_UPPER_DEPS)$(C_DEPS) LCD_LVGL.elf
	-@
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents

-include ../makefile.targets
